#include <iostream>
#include <string>
using namespace std;

int main() 
{
	string x;
    string y;
    cin>>x>>y;
    cout<<x.length()<<" ";
     cout<<y.length()<<endl;
    string z=x+y;
    cout<<z<<endl;
    string e;
   e[0]=x[0];
   x[0]=y[0];
   y[0]=e[0];
   cout<<x<<" "<<y;

    return 0;
 }
