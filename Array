#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a[1000],n,i,x;
    cin>>n;
    for(i=0;i<n;i++) 
    {
        cin>> x;
        a[i]=x;
    } 
    for(i=n-1;i>=0;i--)
    {
        cout<<a[i]<<" ";
    } 
    return 0;
}
